{
    "name": "porject/ddd",
    "type": "project",
    "description": "Studying project with DDD",
    "license": "proprietary",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-mysqli": "*",
        "ext-curl": "*",
        "ext-ctype": "*",
        "nikic/fast-route": "^1.3",
        "symfony/http-foundation": "^6.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10",
        "symfony/var-dumper": "7.0.x-dev"
    },
    "autoload": {
        "psr-4": {
            "App\\": ""
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "test"
        }
    }
}
